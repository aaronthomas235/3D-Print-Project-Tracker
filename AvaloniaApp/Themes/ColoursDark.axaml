<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Color x:Key="BackgroundColour">#2D2D30</Color>
	<Color x:Key="BorderColour">#cc0000</Color> <!--#444444-->
	<Color x:Key="TextColour">#CCCCCC</Color>
	<Color x:Key="PointerOverColour">#3A3A3D</Color>
	<Color x:Key="PointerPressedColour">#007ACC</Color>
	<Color x:Key="PointerPressedTextColour">#FFFFFF</Color>

	<SolidColorBrush x:Key="BackgroundBrush" Color="{DynamicResource BackgroundColour}" />
	<SolidColorBrush x:Key="BorderBrush" Color="{DynamicResource BorderColour}" />
	<SolidColorBrush x:Key="TextBrush" Color="{DynamicResource TextColour}" />
	<SolidColorBrush x:Key="PointerOverBrush" Color="{DynamicResource PointerOverColour}" />
	<SolidColorBrush x:Key="PointerPressedBrush" Color="{DynamicResource PointerPressedColour}" />
	<SolidColorBrush x:Key="PointerPressedTextBrush" Color="{DynamicResource PointerPressedTextColour}" />
</ResourceDictionary>
